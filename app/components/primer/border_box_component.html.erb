<%= primer :base, **@kwargs do %>
  <% if header %>
    <%= primer :base, **header.kwargs do %>
      <%= header.content %>
    <% end %>
  <% end %>
  <% if body %>
    <%= primer :base, **body.kwargs do %>
      <%= body.content %>
    <% end %>
  <% end %>
  <% if rows.any? %>
    <ul>
      <% rows.each do |row| %>
        <%= primer :base, **row.kwargs do %>
          <%= row.content %>
        <% end %>
      <% end %>
    </ul>
  <% end %>
  <% if footer %>
    <%= primer :base, **footer.kwargs do %>
      <%= footer.content %>
    <% end %>
  <% end %>
<% end %>
